{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "SMS-Angular": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": false
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": {
              "base": "dist/sms-angular"
            },
            "allowedCommonJsDependencies": ["moment", "reflect-metadata"],
            "index": "src/index.html",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "src/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              {
                "input": "src/assets/theme/cyan-orange.css",
                "bundleName": "cyan-orange",
                "inject": false
              },
              {
                "input": "src/assets/theme/magenta-violet.css",
                "bundleName": "magenta-violet",
                "inject": false
              },
              {
                "input": "src/assets/theme/indigo-pink.css",
                "bundleName": "indigo-pink",
                "inject": false
              },
              {
                "input": "src/assets/theme/deeppurple-amber.css",
                "bundleName": "deeppurple-amber",
                "inject": false
              },
              {
                "input": "src/assets/theme/pink-bluegrey.css",
                "bundleName": "pink-bluegrey",
                "inject": false
              },
              {
                "input": "src/assets/theme/purple-green.css",
                "bundleName": "purple-green",
                "inject": false
              }
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"
            ],
            "browser": "src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                {
                  "glob": "Web.config",
                  "input": "src",
                  "output": "/"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",

          "configurations": {
            "production": {
              "buildTarget": "SMS-Angular:build:production"
            },
            "development": {
              "buildTarget": "SMS-Angular:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "SMS-Angular:build"
          }
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "src/styles.scss",
              {
                "input": "src/assets/theme/cyan-orange.css",
                "bundleName": "cyan-orange",
                "inject": false
              },
              {
                "input": "src/assets/theme/magenta-violet.css",
                "bundleName": "magenta-violet",
                "inject": false
              },
              {
                "input": "src/assets/theme/indigo-pink.css",
                "bundleName": "indigo-pink",
                "inject": false
              },
              {
                "input": "src/assets/theme/deeppurple-amber.css",
                "bundleName": "deeppurple-amber",
                "inject": false
              },
              {
                "input": "src/assets/theme/pink-bluegrey.css",
                "bundleName": "pink-bluegrey",
                "inject": false
              },
              {
                "input": "src/assets/theme/purple-green.css",
                "bundleName": "purple-green",
                "inject": false
              }
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component",
      "style": "scss",
      "standalone": false
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
